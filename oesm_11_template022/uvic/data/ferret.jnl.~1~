set mode verify
use L_ice.nc
sh da
sh gr
sh gr l_icethk
shade l_icethk[l=1000]
shade l_icethk[l=10]
shade l_icethk[l=20]
shade l_icethk[l=2]
shade l_icethk[l=22]
shade l_icethk[l=1]
plot l_icethk[x=@ave,y@ave]
plot l_icethk[x=@ave,y=@ave]
exit
